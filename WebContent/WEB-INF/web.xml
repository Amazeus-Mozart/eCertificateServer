	</servlet>
	<servlet>
		<servlet-name>OrderDetailServlet</servlet-name>
		<servlet-class>com.eCertificate.servlet.OrderDetailServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PayServlet</servlet-name>
		<servlet-class>com.eCertificate.servlet.PayServlet</servlet-class>
	</servlet>
	<servlet>
