	<servlet-mapping>
		<servlet-name>StartTableServlet</servlet-name>
		<url-pattern>/servlet/StartTableServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OrderDetailServlet</servlet-name>
		<url-pattern>/servlet/OrderDetailServlet</url-pattern>
	</servlet-mapping>
